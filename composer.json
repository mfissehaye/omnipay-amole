{
    "name": "omnipay-amole",
    "description": "Amole driver for the Omnipay payment processing library",
    "keywords": [
        "amole",
        "payment"
        "dashen"
        "ethiopia"
        "gateway"
    ],
    "homepage": "https://github.com/mfissehaye/omnipay-amole",
    "license": "MIT",
    "authors": [
        {
            "name": "Agbonghama Collins",
            "email": "me@w3guy.com",
            "homepage": "http://w3guy.com"
        }
    ],
    "require": {
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "satooshi/php-coveralls": "^0.7.1"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\TwoCheckoutPlus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}